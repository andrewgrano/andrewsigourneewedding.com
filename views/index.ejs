            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="/img/heroCarousel-1.jpg" class="hero__Carousel--img img-responsive" alt="Engagement Photo of Andrew and Sigournee in Venice on a gondola">
                </div>
